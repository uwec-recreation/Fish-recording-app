{{> header}}
        <script src="/js/libs/moment.js"></script>
        <script> var i = 1</script>

        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <!-- /.panel-heading -->
                    <div class="panel-body">
        <table  class="table table-striped table-bordered table-hover " id="dataTables-example">
          <thead>
          <tr>
            <caption id="lastUpdate" style="font-weight:bold">Winners</caption>

            <th>Place</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Fish</th>
            <th>Weight</th>
            <th>Time</th>
            <th>Prize</th>
          </tr>
        </thead>
        <tbody>
          {{#each data}}
          <tr id="row_{{_id}}">
            <td id="place_{{_id}}">
              <script>
                $("#place_{{_id}}").html(i);
                if(i > 25 && i%5 != 0 && i <= 300) {
                  $("#row_{{_id}}").css("display", "none");
                }

                i += 1;

              </script>
            </td>
            <td>{{firstName}}</td>
            <td>{{lastName}}</td>
            <td>{{fish}}</td>
            <td>{{weight}}</td>
            <td>
              <script>
                 var str = moment({{createdAt}}).format('h:mm:ss A');
                 $("td:last").html(str);
              </script>
            </td>
            <td></td>
          </tr>
          {{/each}}
        </tbody>
        </table>
      </div>
    </div>
  </div>
  </div>
</div>



  </body>

</html>
